{
	"manifest_version": 2,
	"name": "WinCleaner",
	"version": "2.3",
	"icons": { "16": "icon_16.png", "128": "icon_128.png" },
	"background": {
		"persistent": false,
		"scripts": [ "background.js" ]
	},
	"permissions": [
		"browsingData",
		"storage",
		"tabs"
	],
	"offline_enabled": true,
	"browser_action": {
		"default_icon": "icon_19.png",
		"default_title": "WinCleaner"
	}
}